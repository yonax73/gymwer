# Routes
# This file defines all application routes (Higher priority routes first)

#loginControl

GET      /login                           com.gymwer.set.controller.login.LoginControl.login()
GET      /signup                          com.gymwer.set.controller.login.LoginControl.signUp()
POST    /createAccount                    com.gymwer.set.controller.login.LoginControl.createAccount()




# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
