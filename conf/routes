# Routes
# This file defines all application routes (Higher priority routes first)

#UserControl

GET      /login                           com.yonaxtics.gymwer.set.user.controller.UserControl.login()
GET      /signup                          com.yonaxtics.gymwer.set.user.controller.UserControl.signUp()
POST    /createAccount                    com.yonaxtics.gymwer.set.user.controller.UserControl.createAccount()
POST    /signIn                           com.yonaxtics.gymwer.set.user.controller.UserControl.signIn()

#GymControl

GET     /home                              com.yonaxtics.gymwer.dpa.gym.controller.GymControl.home()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
