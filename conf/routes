# Routes
# This file defines all application routes (Higher priority routes first)

#UserControl

GET      /login                           com.yonaxtics.gymwer.set.user.controller.UserControl.login()
GET      /signup                          com.yonaxtics.gymwer.set.user.controller.UserControl.signUp()
POST      /signIn                          com.yonaxtics.gymwer.set.user.controller.UserControl.signIn()
GET    /signOut                           com.yonaxtics.gymwer.set.user.controller.UserControl.signOut()
POST    /createAccount                    com.yonaxtics.gymwer.set.user.controller.UserControl.createAccount()


#GymControl

GET     /dashboard                         com.yonaxtics.gymwer.dpa.gym.controller.GymControl.dashboard()
GET     /load                              com.yonaxtics.gymwer.dpa.gym.controller.GymControl.load()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
